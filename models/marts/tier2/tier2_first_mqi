select 
        sm.email
      , sm.id
      , sm.domainType
      , sm.mqi_date
      , sm.csmpaignSourcedBy
      , sm.parentcampaignid
      , sm.parentCampaign
      , sm.parentctatype
      , sm.parentchannel
      , sm.parentCTAsubtype
      , sm.childCsmpaign
      , sm.childCTAtype
      , sm.childChannel
      , sm.childCTAsubtype
      , sm.employeeSegment
      , sm.response
FROM {{ ref('tier2_sorted_mqi') }} as sm
Where 
          sm.rn = 1
      and sm.email not like '%vidyard%'
      and sm.campaign_sourced_by__c in ('Marketing', 'Product')

 